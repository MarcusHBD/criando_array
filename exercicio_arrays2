lista_musicos = [ "Djavan", "Roberto Carlos", "Elis Regina", "Tom Jobim", "Milton Nascimento", 'Chico Buarque', 'Nara Leão', 'Pitty', 'Simonal', 'Moacir Santos', 'Caetano Veloso', 'Elza Soares', 'Paulinho da Viola', 'Yamandú Costa', 'Gal Costa'] 

